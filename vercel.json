{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "api/example.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "api/example.php"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ]
}
